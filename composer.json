{
  "name": "emartech/chunkulator",
  "description": "Package to process large lists in batches",
  "minimum-stability": "stable",
  "license": [
    "MIT"
  ],
  "prefer-stable" : false,
  "require": {
    "php": "~7.3",
    "emartech/php-amqp-wrapper": "1.1.4"
  },
  "require-dev": {
    "emartech/php-test-helpers": "^1.1",
    "phpunit/phpunit": "*",
    "phpunit/php-invoker": "*"
  },
  "autoload": {
    "psr-4": {
      "Emartech\\Chunkulator\\": "src/"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "Emartech\\Chunkulator\\Test\\": "test/"
    }
  }
}
